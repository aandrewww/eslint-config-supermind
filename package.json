{
  "name": "eslint-config-supermind",
  "version": "0.5.0",
  "description": "Supermind's ESLint configuration",
  "homepage": "https://github.com/supermind-hq/eslint-config-supermind#readme",
  "author": "Matthew Wagerfield <matthew@supermind.com>",
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "lint": "eslint .",
    "update-patch": "npm version patch",
    "update-minor": "npm version minor",
    "update-major": "npm version major",
    "list-rules": "eslint-index .eslintrc.js",
    "table-rules": "eslint-index .eslintrc.js --format table",
    "count-rules": "eslint-index .eslintrc.js --format count",
    "list-omitted-rules": "eslint-index .eslintrc.js --status omitted",
    "count-omitted-rules": "eslint-index .eslintrc.js --status omitted --format count",
    "list-enabled-rules": "eslint-index .eslintrc.js --status warn error",
    "count-enabled-rules": "eslint-index .eslintrc.js --status warn error --format count",
    "list-disabled-rules": "eslint-index .eslintrc.js --status omitted off",
    "count-disabled-rules": "eslint-index .eslintrc.js --status omitted off --format count"
  },
  "keywords": [
    "eslint",
    "eslintconfig",
    "config",
    "supermind"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/supermind-hq/eslint-config-supermind.git"
  },
  "bugs": {
    "url": "https://github.com/supermind-hq/eslint-config-supermind/issues"
  },
  "dependencies": {
    "babel-eslint": "^7.1.0",
    "eslint": "^3.10.2",
    "eslint-plugin-flowtype": "^2.25.0",
    "eslint-plugin-import": "^2.0.1",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.4.1"
  },
  "devDependencies": {
    "eslint-index": "^0.6.0"
  },
  "peerDependencies": {
    "eslint": "^3.10.2"
  }
}
